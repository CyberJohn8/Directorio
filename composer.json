{
    "name": "cyberjohn8/directorio",
    "description": "Directorio PHP PWA",
    "type": "project",
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "php": "^8.0 || ^8.1 || ^8.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}